{
    "name": "PsychoXPath",
    "version": "1.0.6",
    "description": "Adds XPath support to the contextual menu.",
    "permissions": [
        "contextMenus",
        "tabs"
    ],
    "icons": {
        "16": "logo_16x16.png",
        "48": "logo_48x48.png",
        "128": "logo_128x128.png"
    },
    "background_page": "background.html",
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "js": [
            "content.js",
            "psychoxpath.js"
         ]
    }]
}
